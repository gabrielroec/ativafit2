const updateBundleTitle = () => {
  const title = document.querySelector('.cbb-frequently-bought-title');
  if (title && title.textContent !== 'Maximize your results — combine with these essentials.') {
    title.textContent = 'Maximize your results — combine with these essentials.';
    console.log('[✔] Título atualizado.');
  }
