{% schema %}
  {
    "name": "Collection Features",
    "settings": [
      {
        "type": "image_picker",
        "id": "icon_1",
        "label": "Feature 1 Icon (optional)",
        "info": "Upload an image for feature 1 (82x76 recommended). If blank, default SVG will be used."
      },
