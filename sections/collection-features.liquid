{%- style -%}
  .collection-features {
    padding: 40px 0;
    background: #fff;
  }

  .collection-features__container {
    max-width: 1040px;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    gap: 20px;
    padding: 0 20px;
  }

  .collection-features__item {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
  }

  .collection-features__icon {
    margin-bottom: 20px;
  }

  .collection-features__title {
    font-size: 24px;
    font-weight: 600;
    margin-bottom: 10px;
    color: #000;
  }

  @media screen and (max-width: 768px) {
    .collection-features__container {
      flex-direction: column;
      gap: 40px;
    }

    .collection-features__item {
      width: 100%;
    }
  }
{%- endstyle -%}

<section class="collection-features">
  <div class="collection-features__container">
    <div class="collection-features__item">
      <div class="collection-features__icon">
        {% if section.settings.icon_1 != blank %}
          {{ section.settings.icon_1 | image_url: width: 82 | image_tag: class: 'collection-features__icon-image', loading: 'lazy', alt: section.settings.title_1 }}
          {% else %}
          <!-- Fallback SVG -->
          <svg
            width="82"
            height="76"
            viewBox="0 0 82 76"
            fill="none"
            xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink">
            <rect
              x="0.739258"
              y="2"
              width="72"
              height="72"
              rx="36"
              fill="#EB701F"
              fill-opacity="0.4" />
            <rect
              x="6.26074"
              y="1"
              width="74"
              height="74"
              rx="37"
              fill="#EB701F"
              stroke="white"
              stroke-width="2" />
            <rect
              x="26"
              y="21"
              width="34"
              height="34"
              fill="url(#pattern0_540_2413)" />
            <defs>
              <pattern
                id="pattern0_540_2413"
                patternContentUnits="objectBoundingBox"
                width="1"
                height="1">
                <use xlink:href="#image0_540_2413" transform="scale(0.002)" />
              </pattern>
              <image
                id="image0_540_2413"
                width="500"
                height="500"
                preserveAspectRatio="none"
                xlink:href="data:image/png;base64," />
            </defs>
          </svg>


        {% endif %}
      </div>
      <h3 class="collection-features__title bold">{{ section.settings.title_1 }}</h3>
