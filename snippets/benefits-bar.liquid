{% style %}
  .benefits-bar {
    background: #E7E7E7;
    border-radius: 8px;
    padding: 20px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20px 0;
    gap: 20px;
  }

  .benefit-item {
    display: flex;
    align-items: center;
    flex-direction: column;
    gap: 12px;
    flex: 1;
    justify-content: center;
  }

  .benefit-icon {
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .benefit-text {
    display: flex;
    flex-direction: column;
    font-family: 'Barlow', sans-serif;
    text-align: center;
    gap: 0;
  }

  .benefit-text strong {
    font-size: 14px;
    color: #000;
    margin: 0;
  }

  .benefit-text span {
    font-size: 12px;
    color: #666;
  }

  @media screen and (max-width: 768px) {}
{% endstyle %}