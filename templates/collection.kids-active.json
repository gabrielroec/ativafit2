        "tab_1dK3s8": {
          "type": "tab",
          "settings": {
            "title": "Safety-First Protection",
            "image": "shopify://shop_images/Safety-First_Protection-trampoline-kids_active.webp",
            "text": "<p>Every kids trampoline in our lineup includes a No-Gap jumping mat and a Safe Pin locking system, providing a 100% secure toddler trampoline experience.</p>"
